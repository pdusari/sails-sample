	<div class="row"> 
        <div class="col-md-12">
        <h4>Bootstrap Snipp for Datatable</h4>
        <div class="table-responsive">
              <table id="mytable" class="table table-bordred table-striped">
                   
                   <thead>
                   <th>First Name</th>
                     <th>Email</th>
                      <th>Edit</th>             
                       <th>Delete</th>
                   </thead>
    <tbody>
    <% _.each(users,function(user){ %>
    <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><p data-placement="top" data-toggle="tooltip" title="Edit"><a href="/user/edit?id=<%= user.id %>">Edit</a></p></td>
    <td><p data-placement="top" data-toggle="tooltip" title="Delete"><a href="/user/delete?id=<%= user.id %>">Delete</a></p></td>
    </tr>
    <% }); %>
    </tbody>
        
</table>

<div class="clearfix"></div>
<ul class="pagination pull-right">
  <li class="disabled"><a href="#"><span class="glyphicon glyphicon-chevron-left"></span></a></li>
  <li class="active"><a href="#">1</a></li>
  <li><a href="#">2</a></li>
  <li><a href="#">3</a></li>
  <li><a href="#">4</a></li>
  <li><a href="#">5</a></li>
  <li><a href="#"><span class="glyphicon glyphicon-chevron-right"></span></a></li>
</ul>
                
            </div>
            
        </div>
	</div>
